
<div class='row'>
  <div class='col-md-4 col-md-offset-4'>
    <table class='tag-show-table-list'>
      <tr>
        <td>
          <h1 class='text-center'><%= @tag.name %></h1>
        </td>
      </tr>
      <tr>
        <td>
          <ul>
        <% @players.each do |p| %>
            <li><%= link_to p.name, player_path(p.id) %></li>
        <% end %>
          </ul>
        </td>
      </tr>
    </table>
  </div>
</div>